{
  "name": "nexlab",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "20.x",
    "npm": ">=10.2.3"
  },
  "scripts": {
    "dev": "npm run setup-pdf-worker && vite",
    "build": "npm run setup-pdf-worker && vite build",
    "setup-pdf-worker": "mkdir -p public/js && cp node_modules/pdfjs-dist/build/pdf.worker.min.mjs public/js/pdf.worker.min.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "type-check": "tsc --noEmit --skipLibCheck || echo 'TypeScript check completed with errors - proceeding with build'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "homepage": "https://institute-for-future-intelligence.github.io/nexlab/",
  "author": {
    "name": "Andriy Kashyrskyy",
    "url": "https://institute-for-future-intelligence.github.io/nexlab/"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^44.3.0",
    "@ckeditor/ckeditor5-build-multi-root": "^44.3.0",
    "@ckeditor/ckeditor5-editor-inline": "^44.3.0",
    "@ckeditor/ckeditor5-react": "^11.0.0",
    "@ckeditor/ckeditor5-watchdog": "^46.1.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@google/generative-ai": "^0.24.1",
    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^5.18.0",
    "@mui/x-date-pickers": "^8.11.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^20.12.10",
    "@types/pdf-parse": "^1.1.5",
    "browser-image-compression": "^2.0.2",
    "domelementtype": "^2.3.0",
    "domhandler": "^5.0.3",
    "dompurify": "^3.1.0",
    "domutils": "^3.2.2",
    "firebase": "^9.23.0",
    "firebase-admin": "^12.2.0",
    "firebase-functions": "^5.0.1",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.10.3",
    "htmlparser2": "^10.0.0",
    "jspdf": "^3.0.0",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "mammoth": "^1.9.1",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^5.4.54",
    "pizzip": "^3.2.0",
    "rag-chatbot-interface-ifi": "^1.0.5",
    "rag-quiz-modal-ifi": "^1.0.1",
    "react": "^19.0.0",
    "react-datepicker": "^7.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-image-file-resizer": "^0.4.8",
    "react-router-dom": "^6.22.3",
    "tinymce": "^7.0.0",
    "uuid": "^9.0.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@types/domhandler": "^3.1.0",
    "@types/dompurify": "^3.0.5",
    "@types/domutils": "^1.7.8",
    "@types/htmlparser2": "^3.10.7",
    "@types/lodash": "^4.17.1",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-legacy": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^1.4.0",
    "@vitest/ui": "^1.4.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^4.0.0",
    "jsdom": "^24.0.0",
    "typescript": "^5.4.5",
    "vite": "^6.0.3",
    "vitest": "^1.4.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
